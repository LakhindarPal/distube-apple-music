{
  "parserOptions": {
    "project": "./tsconfig.eslint.json"
  },
  "rules": {
    "valid-jsdoc": "off",
    "jsdoc/check-tag-names": "off"
  },
  "extends": "distube",
  "ignorePatterns": [
    "**/node_modules/",
    ".git/",
    "**/dist/",
    "**/template/",
    "**/coverage/",
    "**/storybook-static/",
    "**/.next/"
  ]
}
